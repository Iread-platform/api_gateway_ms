{
	"Routes": [
		{
			"UseServiceDiscovery": true,
			"ServiceName": "story_ms",
			"DownstreamPathTemplate": "/api/stories/{anything}",
			"UpstreamPathTemplate": "/api/iread/stories/{anything}",
			"DownstreamScheme": "http",
			"UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
			"ReRoutesCaseSensitive": false,
			"LoadBalancerOptions": {
				"Type": "RoundRobin"
			}
		}
	],
	"GlobalConfiguration": {
		"ServiceDiscoveryProvider": {
			"Scheme": "http",
			"Host": "127.0.0.1",
			"Port": 8500,
			"Type": "Consul"
		}
	}
}


